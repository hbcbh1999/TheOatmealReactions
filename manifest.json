{
  "name": "The Oatmeal Reactions for Facebook",
  "short_name": "The Oatmeal Reactions",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Replaces Facebook Reactions with The Oatmeal pictures.",
  "homepage_url": "http://students.washington.edu/parsis/portfolio/",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "permissions": [
    "*://*.facebook.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.facebook.com/*"
      ],
      "css": [
        "content_script/replace-reactions.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "img/*.png"
  ]
}
